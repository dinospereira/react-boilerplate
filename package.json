        "husky": "^4.2.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^25.4.0",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "react-axe": "^3.4.1",
        "style-loader": "^1.1.4",
        "webpack": "^5.75.0",
        "webpack-bundle-analyzer": "^3.7.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^3.10.3",
        "webpack-merge": "^4.2.2"
